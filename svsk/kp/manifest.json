    "gymnasium_2011_sv": {
      "name": "2011 Gymnasium curriculum plans in Swedish",
      "description": "Gymnasium årskurs 1 till 3",
      "language": "sv",
      "year": 2011,
      "school_level": "gymnasium",
      "grades": "1-3",
      "general_documents": [],
      "subjects": {
        "common_subjects": {
          "description": "Subjects common to all gymnasium programs",
          "subjects": {
            "svenska": {
              "subject_id": "SVE",
              "subject_name": "Svenska",
              "subject_name_en": "Swedish",
              "curriculum_url": "https://www.skolverket.se/sitevision/proxy/undervisning/gymnasieskolan/program-och-amnen-i-gymnasieskolan/hitta-program-amnen-och-kurser-i-gymnasieskolan-gy11/amne/svid12_5dfee44715d35a5cdfa92a3/907561864/subject/SVE/8/pdf",
              "html_file": "placeholder_svenska_gymnasium_2011.html",
              "version": 8,
              "course_structure": {
                "type": "separate_courses",
                "courses": ["Svenska 1", "Svenska 2", "Svenska 3", "Litteratur", "Retorik", "Skrivande"]
              },
              "content_analysis": {
                "main_purpose": "Undervisningen i ämnet svenska ska syfta till att eleverna utvecklar sin förmåga att kommunicera i tal och skrift samt att läsa och arbeta med texter, både skönlitteratur och andra typer av texter. Eleverna ska ges möjlighet att i skönlitteraturen se såväl det särskiljande som det allmänmänskliga i tid och rum.",
                "core_focus": ["kommunikativ förmåga", "textmångfald", "litterärt perspektiv"],
                "approach": "Mer traditionell formulering, fokus på litterär tolkning",
                "structural_characteristics": {
                  "progression": "linjär (Svenska 1 → 2 → 3)",
                  "literature_focus": "tydlig tyngd på litteraturhistoria och klassiska genrer",
                  "scientific_texts": "vetenskaplig text och retorik först på avancerad nivå (Svenska 3)",
                  "language_history": "placeras i slutet (Svenska 3)",
                  "digitalization": "i första hand som verktyg för textbearbetning/presentation"
                },
                "extracted_date": "2025-08-28"
              },
              "central_content": {
                "svenska_1": {
                  "areas": [
                    "Muntlig framställning (mottagaranpassning, respons, digitala hjälpmedel)",
                    "Skriftlig framställning (argumentation, språkriktighet, digital textbearbetning)",
                    "Retoriska grunder",
                    "Textanalys, källkritik, upphovsrätt",
                    "Skönlitteratur från olika tider/kulturer",
                    "Berättarteknik, genrer, stil",
                    "Språkliga begrepp och variation (sociolekt, dialekt, kön, ålder, digitalisering)"
                  ]
                },
                "svenska_2": {
                  "areas": [
                    "Muntlig framställning (utredande, argumenterande)",
                    "Skriftlig framställning (utredande/argumenterande texter)",
                    "Referat, källkritik, normer och stildrag",
                    "Svenska och internationella författarskap, inkl. dansk/norsk litteratur",
                    "Litteratur och samhällsutveckling",
                    "Språkets uppbyggnad (grammatik)",
                    "Språkförhållanden i Sverige/Norden (minoritetsspråk, lagstiftning, dialekter)"
                  ]
                },
                "svenska_3": {
                  "areas": [
                    "Muntlig framställning (fördjupad retorik)",
                    "Vetenskapliga texter (disposition, stil, källkritik)",
                    "Litteraturanalys (prosa, lyrik, dramatik, litteraturvetenskapliga begrepp)",
                    "Språkhistoria (ursprung, utveckling, släktskap, språkförändring)"
                  ]
                },
                "optional_courses": {
                  "litteratur": [
                    "Epoker, centrala verk, genrer",
                    "Litteratur i relation till samhälle, kultur, konstformer",
                    "Etik, censur, upphovsrätt",
                    "Djupanalys (tematik, symbolik, stilmedel, jämförelser, perspektivval)"
                  ],
                  "retorik": [
                    "Retoriska arbetsprocessen (intellectio–actio)",
                    "Praktiska tillämpningar (tal, debatt, analys)",
                    "Yttrandefrihet, etik, censur"
                  ],
                  "skrivande": [
                    "Skönlitterärt och journalistiskt skrivande",
                    "Retorisk arbetsprocess",
                    "Spänning fakta–fiktion, tradition–innovation",
                    "Multimodala texter",
                    "Textkritik, språklig medvetenhet, grafisk utformning"
                  ]
                }
              },
              "cross_reference": {
                "english_version": "gymnasium_2011_en.svenska",
                "updated_version": "gymnasium_2025_sv.svenska"
              }
            },
